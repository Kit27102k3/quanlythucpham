{
  "routes": [
    { "src": "/Logo.png", "dest": "/Logo.png" },
    { "src": "/(.*)\\.css", "headers": { "content-type": "text/css" } },
    { "src": "/(.*)\\.js", "headers": { "content-type": "application/javascript" } },
    { "src": "/(.*)\\.json", "headers": { "content-type": "application/json" } },
    { "src": "/(.*)\\.png", "headers": { "content-type": "image/png" } },
    { "src": "/(.*)\\.jpg", "headers": { "content-type": "image/jpeg" } },
    { "src": "/(.*)\\.svg", "headers": { "content-type": "image/svg+xml" } },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|jpg|jpeg|png|gif|ico|svg|woff2|ttf)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
  